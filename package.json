{
  "name": "{{fullName}}",
  "version": "0.0.0-semantic-release",
  "scripts": {
    "cover": "--overwrite-- c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "test": "ava --timeout 2m",
    "docs": "--overwrite-- documentation readme {{main}} --section=API",
    "lint": "--overwrite-- documentation lint {{main}}"
  },
  "devDependencies": {
    "semantic-release": "^17.0.8",
    "c8": "^7.2.0",
    "ava": "^3.9.0",
    "documentation": "^13.0.1"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/tmpls.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/tmpls/issues"
  },
  "homepage": "https://github.com/arlac77/tmpls#readme",
  "template": {
    "inheritFrom": "arlac77/template-node-app"
  }
}